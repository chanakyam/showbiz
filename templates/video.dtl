    </a>
    </noscript>
  </div>
  <span class="clear"></span>
  </div>
  
  
 
  
  <div class="module">
    <div class="module-title"><span>Latest Gossips</span></div>
    {% for n in latestvideos %}
      <div class="video_gallery">
        <a href="/video?id={{n.id}}" >
          <img src="http://newscdn.lycos.com/world_news/{{ n.thumb_image }}" title="{{ n.title }}">  
          <span class="videotext"></span>
      </div>
    {% endfor %}
      <span class="clear"></span>
      <p class="margin20_T"><a href="/more_videos" class="right more">More Videos</a></p>
  <!-- showbiz Interviews module end -->
   <div class="module">
    <div class="module-title"><span>Music</span></div>
      <div class="gallery">
      <!-- <ul>
        {% for n in galleryimgs %}
